<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../partials/head.ejs') %>
    </head>
<body>
    <div>
        <%-include('../partials/header.ejs') %>
    </div>
    <header>
        <h1 class="">Overbooked Main Page</h1>
    </header>
    <div class="card-deck container-fluid justify-content-around text-center row">
        <% for (let i = 0; i < book.length; i++) { %>
                <div class="card col flex owned<%= book[i].owned %> read<%= book[i].read %>" style="min-width: 300px; max-width: 300px;">
                    <a href="/bookworm/<%= book[i]._id %>" class="">
                        <img src="<%= `${book[i].img}` %>" class="rounded card-img-top" style="object-fit: cover;" >
                    </a>
                    <div class="card-body">
                        <h6 class="card-title"><%= book[i].name %></h6>
                    </div>
                </div>
        <% } %>
    </div>
    <footer>
        <%- include('../partials/footer.ejs') %>
    </footer>
</body>
        <%- include('../partials/jquery.ejs') %>
</html>